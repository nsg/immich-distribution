#!/bin/bash

. $SNAP/bin/load-env

if [ -z $DB_PASSWORD ]; then
    echo "NOTE: DB_PASSWORD not set, server will not be started"
    sleep 2
    exit 1
fi

banner

cd $SNAP/usr/src/app
node dist/apps/immich/apps/immich/src/main

  #     #DB_PORT: 5432
  #     # REDIS_PORT=6379
  #     # REDIS_DBINDEX=0
  #     # REDIS_PASSWORD=
  #     # REDIS_SOCKET=
